{
  "name": "ab-assistant",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve-hu": "ng serve --configuration=hu --host=0.0.0.0",
    "server": "http-server -c-1 dist\\ab-assistant",
    "extract-i18n": "ng xi18n --output-path localization && xliffmerge --profile xliffmerge.json en hu",
    "build-prd": "./node_modules/.bin/ng build --prod",
    "build-dev": "./node_modules/.bin/ng build --configuration=dev",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng"
  },
  "private": true,
